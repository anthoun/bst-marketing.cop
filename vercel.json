{
    "framework": "vite",
    "installCommand": "npm install",
    "buildCommand": "npm run build",
    "redirects": [
        {
            "source": "/free-marketing-analysis",
            "destination": "/contact",
            "permanent": true
        },
        {
            "source": "/free-marketing-analysis/",
            "destination": "/contact",
            "permanent": true
        }
    ],
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}